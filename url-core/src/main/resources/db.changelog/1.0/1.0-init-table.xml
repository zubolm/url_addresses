<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet author="b3user" id="420">
        <createTable tableName="URL_ADDRESS">
            <column name="id" type="INTEGER" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="PK_url_id"/>
            </column>
            <column name="URL" type="VARCHAR2(500 BYTE)"/>
            <column name="CREATED_DATE" type="TIMESTAMP(6) WITH TIME ZONE"/>
        </createTable>
    </changeSet>

</databaseChangeLog>